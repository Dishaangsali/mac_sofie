pipeline {
    agent any 
    stages {
        stage('Checkout from SCM') {
            steps {
                echo "Mac"
            }
        }
        stage('Build') {
            steps {
                echo "Sophie"
            }
        }
        stage('Run') {
            steps {
                echo "Jimmy"
            }
        }
        stage('Test') {
            steps {
                echo "Scotchy"
            }
        }
        stage('Deploy') {
            steps {
                echo "Tonu"
            }
        }
    }
}
